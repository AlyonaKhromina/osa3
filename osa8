#include <iostream>

using namespace std;


class Stack {// СТЕК
private:
	int* myStack;
	int head = 0, inOps = 0, outOps = 0, size;
	enum states { empty, full, ok };
	states checkResult;
	int toReturn;

	states fullOrEmpty() { // проверка, полный или пустой
		if (inOps == outOps) {
			return states::empty;
		}
		else if (inOps - outOps == size) {
			return states::full;
		}
		else
			return states::ok;
	}

	void translateHead() { // перемещает место для записи числа (голову)
		if (head > size)
			head = 0;
	}

public:
	Stack(int arraySize) { // конструктор :)
		size = arraySize;
		myStack = new int[size];
	}

	~Stack() { // деконструктор :(
		delete[] myStack;
	}

	bool is_empty() { // проверка стека на пустоту, костыль, ведь у нас функция уже есть
		if (inOps == outOps) {
			return true;
		}
		else
			return false;
	}

	void add(int arg) { // добавление
		translateHead();
		checkResult = fullOrEmpty();
		if (checkResult != states::full) {
			inOps++;
			myStack[head] = arg;
			head++;
		}
	}

	int out() { // забор числа и его удаление 
		checkResult = fullOrEmpty();
		if (checkResult != states::empty) {
			outOps++;
			toReturn = myStack[head - 1];
			myStack[head - 1] = 0;
			head--;
			return toReturn;
		}
		return -1;
	}

};

class Graph { // 
	int** matrix; // матрица смежности
	bool* visited; // посещённые точки
	int* ways; // пути, куда можем пойти
	int start, end; // начальная и конечная точка
	int size;  // размер матрицы

public:
	Graph(int N, int start, int end) {  // конструктор
		
		matrix = new int*[N];  // создаем матрицу смежности
		for (int j = 0; j < N; j++) {
			int* temp = new int[N];
			for (int i = 0; i < N; i++) {
				temp[i] = 0;
			}
			matrix[j] = temp;
		}
		size = N;

		visited = new bool[N]; // массив посещённых
		for (int o = 0; o < N; o++) {
			visited[o] = 0;
		}
		ways = new int[N]; // массив путей
		for (int u = 0; u < N; u++) {
			ways[u] = 0;
		}

		this->start = start;
		this->end = end;
	}

	void add_edge(int from, int to) { // добавляет путь
		matrix[from][to] = 1;
	}

	int* find_ways() {
		Stack Que(size);
		Que.add(start); // добавляем в очередь начальную точку
		visited[start] = 1;
		ways[start] = -1;
		while (!Que.is_empty())  // пока очередь не пустая
		{
			int unit = Que.out();  // берём элемент из очереди
			for (int i = 0; i < size; i++)
			{
				if ((visited[i] == 0) && (matrix[unit][i] == 1)) // есть ли связь и посещяли ли мы точку
				{ 
					Que.add(i); // добавляем в очередь
					ways[i] = unit; // добавляем в путь нашу точку
					visited[i] = 1; // теперь эта точка посещена
				}
			}
		}
		int dne = end;
		int count = 0;
		do {
			count++;
			dne = ways[dne];
		} while (dne != ways[start]); // высчитываем, сколько шагов нужно сделать до конечной точки
		int* to_return = new int[count + 1];
		to_return[0] = count;
		int i = 0;
		to_return[i + 1] = end;
		i++;
		do {
			to_return[i + 1] = ways[end]; // записываем в массив точки, по которым мы прошли
			i++;
			end = ways[end];
		} while (end != start);
		return to_return;
	}

	void test() {
		for (int i = 0; i < size; i++) {
			for (int j = 0; j < size; j++) {
				cout << "  " << matrix[i][j] << "  ";
			}
			cout << endl;
		}
	}
};

void print(int* arr) {
	for (int i = arr[0]; i > 0; i--) {
		cout << arr[i] << " ";
	}
	cout << endl << endl;
}

int main(int count, char** params)
{

	setlocale(LC_ALL, "");

	Graph g(atoi(params[1]), atoi(params[2]), atoi(params[3]));

	int counter = 4;
	while (counter <= count - 1) {
		if (string(params[counter]) == "add") {
			g.add_edge(atoi(params[counter + 1]), atoi(params[counter + 2]));
			counter += 3;
		}
	}
	g.test();
	print(g.find_ways());
	
	system("pause");
	return 0;
}
